{
  "name": "be-certcontrol",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "nodemon",
    "build": "npx ts-node build.ts && tree -L 3 dist -a -I './*.js.map'",
    "start": "node -r module-alias/register ./dist --env=production",
    "lint": "npx eslint src/**/*.ts --fix",
    "format": "npx prettier src/**/*.ts --write"
  },
  "keywords": [],
  "author": "Blitz IT Solutions <development@beblitz.com.br>",
  "license": "ISC",
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "./node_modules/.bin/ts-node --files -r tsconfig-paths/register ./src"
  },
  "_moduleAliases": {
    "@app": "dist/app",
    "@config": "dist/config",
    "@controllers": "dist/controllers",
    "@core": "dist/core",
    "@declarations": "dist/declarations",
    "@interfaces": "dist/interfaces",
    "@middlewares": "dist/middlewares",
    "@models": "dist/models",
    "@scripts": "dist/scripts",
    "@services": "dist/services",
    "@types": "dist/types",
    "@utils": "dist/utils"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "command-line-args": "^5.2.1",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.3",
    "fs-extra": "^10.1.0",
    "helmet": "^6.0.0",
    "jet-logger": "^1.2.6",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "restify": "^8.3.2",
    "restify-cors-middleware": "^1.1.1",
    "restify-plugins": "^1.6.0",
    "tsyringe": "^4.7.0",
    "typeorm": "^0.3.10",
    "typescript": "^4.9.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/command-line-args": "^5.2.0",
    "@types/dotenv": "^8.2.0",
    "@types/fs-extra": "^9.0.13",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.9",
    "@types/restify": "^8.5.5",
    "@types/restify-cors-middleware": "^1.0.2",
    "@types/restify-plugins": "^1.5.13",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^1.19.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "tree-node-cli": "^1.6.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0"
  }
}
